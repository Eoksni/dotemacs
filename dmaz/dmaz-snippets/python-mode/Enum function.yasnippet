# -*- mode: snippet -*-
# name: Enum function
# key: enumfunc
# --
def enum(**enums):
#usage Numbers = enum(ONE=1, TWO=2, THREE='three')
    return type('Enum', (), enums)
def enum_auto(*sequential, **named):
#usage Numbers = enum_auto('ZERO', 'ONE', 'TWO')
    enums = dict(zip(sequential, range(len(sequential))), **named)
    return type('Enum', (), enums)
